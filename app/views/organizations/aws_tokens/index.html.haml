%h3 AWS tokens
= link_to "create", new_organizations_aws_token_path, :class => 'btn'

%table.table.table-striped
  %thead
    %th ID
    %th URL
    %th Duration
    %th
  %tbody
    - @tokens.each do |t|
      %tr
        %td= t.id
        %td= link_to "open", t.link
        %td= t.period
        %td
          = delete_button "Delete", organizations_aws_token_path(t), :class => "btn btn-danger"


