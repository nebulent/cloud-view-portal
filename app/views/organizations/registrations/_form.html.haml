= render 'validation_errors'
= form_for resource, url: registration_path(resource) do |f|
  = fields_for :organization do |org|
    = org.label :name
    = org.text_field :name
  = f.label :email
  = f.text_field :email
  = f.label :password
  = f.password_field :password
  = f.label :password_confirmation
  = f.password_field :password_confirmation
  = f.submit 'Signup'
