= render 'validation_errors'
%br
= form_for resource, url: organizations_user_registration_path do |f|
  = f.label :email
  = f.text_field :email
  = f.label :password
  = f.password_field :password
  = f.label :password_confirmation
  = f.password_field :password_confirmation
  %br
  = f.submit 'Register user', 'class' => 'btn btn-primary'
