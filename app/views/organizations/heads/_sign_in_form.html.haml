= form_for 'head', url: head_session_path do |f|
  = f.label :email
  = f.text_field :email
  = f.label :password
  = f.password_field :password
  .inline= f.check_box :remember_me
  .inline= f.label :remember_me
  = f.submit 'Sign in'
  %p= link_to "Forgot your password?", new_password_path('head')
